    code-unmerged = "!~/.git_aliases/code-unmerged/script.sh"
    con = "!~/.git_aliases/con_/script.sh"  # Suffixed with "_" as Windows folder name can't be "con".
    cone = "!~/.git_aliases/cone/script.sh"
    conea = "!~/.git_aliases/conea/script.sh"
    cum = "!~/.git_aliases/cum/script.sh"
    cumane = "!~/.git_aliases/cumane/script.sh"
    delete-this = "!~/.git_aliases/delete-this/script.sh"
    dump = "!~/.git_aliases/dump/script.sh"
    f = "!~/.git_aliases/f/script.sh"
    ff = "!~/.git_aliases/ff/script.sh"
    git = "!~/.git_aliases/git/script.sh"
    link = "!~/.git_aliases/link/script.sh"
    merge-this = "!~/.git_aliases/merge-this/script.sh"
    pop = "!~/.git_aliases/pop/script.sh"
    pull-all = "!~/.git_aliases/pull-all/script.sh"
    pull-force = "!~/.git_aliases/pull-force/script.sh"
    pull-pr = "!~/.git_aliases/pull-pr/script.sh"
    pullu = "!~/.git_aliases/pullu/script.sh"
    re = "!~/.git_aliases/re/script.sh"
    rebase-from = "!~/.git_aliases/rebase-from/script.sh"
    rebase-preserve = "!~/.git_aliases/rebase-preserve/script.sh"
    rebun = "!~/.git_aliases/rebun/script.sh"
    replace-with = "!~/.git_aliases/replace-with/script.sh"
    redoc = "!~/.git_aliases/redoc/script.sh"
    show-stopped = "!~/.git_aliases/show-stopped/script.sh"
