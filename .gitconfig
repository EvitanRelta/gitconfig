[core]
	editor = "~/.git_aliases/.git_editor.sh"
	autocrlf = input
[user]
	email = shauntanzongzhi@gmail.com
	name = EvitanRelta
[rebase]
	autosquash = true
[alias]
    ab = "!~/.git_aliases/ab/script.sh"
    addn = "!~/.git_aliases/addn/script.sh"
    branch-d-both = "!~/.git_aliases/branch-d-both/script.sh"
