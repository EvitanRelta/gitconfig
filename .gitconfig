    get-stopped-hash = "!~/.git_aliases/.helper_functions/get-stopped-hash/script.sh"
    get-unmerged-paths = "!~/.git_aliases/.helper_functions/get-unmerged-paths/script.sh"
    has-unmerged = "!~/.git_aliases/.helper_functions/has-unmerged/script.sh"
    is-ancestor = "!~/.git_aliases/.helper_functions/is-ancestor/script.sh"
    is-cherry-picking = "!~/.git_aliases/.helper_functions/is-cherry-picking/script.sh"
    is-merging = "!~/.git_aliases/.helper_functions/is-merging/script.sh"
    is-rebasing = "!~/.git_aliases/.helper_functions/is-rebasing/script.sh"
