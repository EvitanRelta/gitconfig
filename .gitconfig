    st = "!~/.git_aliases/st/script.sh"
    stash-staged = "!~/.git_aliases/stash-staged/script.sh"
    stash-unstaged = "!~/.git_aliases/stash-unstaged/script.sh"
    undoc = "!~/.git_aliases/undoc/script.sh"
    unmerge = "!~/.git_aliases/unmerge/script.sh"


